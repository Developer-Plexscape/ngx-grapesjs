{
  "name": "ngx-grapesjs",
  "version": "1.1.0",
  "description": "Angular wrapper library for GrapesJS",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/collection.json ../../dist/ngx-grapesjs/"
  },
  "peerDependencies": {
    "@angular/common": "^16.0.0",
    "@angular/core": "^16.0.0",
    "grapesjs": "^0.21.1",
    "grapesjs-preset-newsletter": "^1.0.1",
    "grapesjs-preset-webpage": "^1.0.2",
    "grapesjs-blocks-basic": "^1.0.1"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Developer-Plexscape/ngx-grapesjs.git"
  },
  "author": "Plexscape",
  "bugs": {
    "url": "https://github.com/Developer-Plexscape/ngx-grapesjs/issues"
  },
  "homepage": "https://github.com/Developer-Plexscape/ngx-grapesjs#readme",
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": "dependencies"
  },
  "devDependencies": {
    "copyfiles": "file:../../node_modules/copyfiles",
    "typescript": "file:../../node_modules/typescript"
  },
  "sideEffects": false,
  "keywords": [
    "angular",
    "grapesjs"
  ]
}
